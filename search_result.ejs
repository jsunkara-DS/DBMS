<h1>
	Search for Product
</h1>
<table border=2>
	<tr style='text-align:left; background-color:#CCC'>
		<th>Product ID</th>
		<th>Type</th>
		<th>Model</th>
		<th>Price</th>
	</tr>
<% data.forEach(function(electronics){ %>
	<tr>
	<td> <%=electronics.e_id %> </td>
	<td> <%=electronics.type %> </td>
	<td> <%=electronics.model %> </td>
	<td> <%=electronics.price%> </td>
	<% }) %>
	</tr></table>


<form method="POST" action="/search">
	Product Type:<input type="text" name="Type"
	placeholder="Enter  Product Type">
	<button>
		Search
	</button>
 <a href="https://sj-sql.run-us-west2.goorm.io">HOME</a>
</form>